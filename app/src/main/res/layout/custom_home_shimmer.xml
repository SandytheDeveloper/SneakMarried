<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_white"
    android:paddingHorizontal="@dimen/_20sdp">

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_30sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:placeholderImage="@color/color_base_grey"
        app:roundAsCircle="true" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView2"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/simpleDraweeView"
        app:placeholderImage="@color/color_base_grey"
        app:roundAsCircle="true" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView3"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_marginStart="@dimen/_5sdp"
        app:layout_constraintStart_toEndOf="@+id/simpleDraweeView2"
        app:layout_constraintTop_toTopOf="@+id/simpleDraweeView2"
        app:placeholderImage="@color/color_base_grey"
        app:roundedCornerRadius="@dimen/_2sdp" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_200sdp"
        android:layout_marginTop="@dimen/_10sdp"
        app:layout_constraintTop_toBottomOf="@+id/simpleDraweeView3"
        app:placeholderImage="@color/color_base_grey"
        app:roundedCornerRadius="@dimen/_8sdp"
        tools:layout_editor_absoluteX="26dp" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView5"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_30sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/simpleDraweeView4"
        app:placeholderImage="@color/color_base_grey"
        app:roundAsCircle="true" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView6"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_marginStart="@dimen/_5sdp"
        app:layout_constraintStart_toEndOf="@+id/simpleDraweeView5"
        app:layout_constraintTop_toTopOf="@+id/simpleDraweeView5"
        app:placeholderImage="@color/color_base_grey"
        app:roundedCornerRadius="@dimen/_2sdp" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/simpleDraweeView6"
        tools:layout_editor_absoluteX="28dp">

        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp"
            app:placeholderImage="@color/color_base_grey"
            app:roundedCornerRadius="@dimen/_8sdp" />

        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp"
            android:layout_marginTop="@dimen/_10sdp"
            app:placeholderImage="@color/color_base_grey"
            app:roundedCornerRadius="@dimen/_8sdp" />

        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp"
            android:layout_marginTop="@dimen/_10sdp"
            app:placeholderImage="@color/color_base_grey"
            app:roundedCornerRadius="@dimen/_8sdp" />

    </LinearLayout>


    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView10"
        android:layout_width="@dimen/_15sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_30sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:placeholderImage="@color/color_base_grey"
        app:roundAsCircle="true" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/simpleDraweeView11"
        android:layout_width="@dimen/_80sdp"
        android:layout_height="@dimen/_15sdp"
        android:layout_marginStart="@dimen/_5sdp"
        app:layout_constraintStart_toEndOf="@+id/simpleDraweeView10"
        app:layout_constraintTop_toTopOf="@+id/simpleDraweeView10"
        app:placeholderImage="@color/color_base_grey"
        app:roundedCornerRadius="@dimen/_2sdp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/simpleDraweeView11"
        tools:layout_editor_absoluteX="28dp">

        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp"
            app:placeholderImage="@color/color_base_grey"
            app:roundedCornerRadius="@dimen/_8sdp" />

        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp"
            android:layout_marginTop="@dimen/_10sdp"
            app:placeholderImage="@color/color_base_grey"
            app:roundedCornerRadius="@dimen/_8sdp" />

        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp"
            android:layout_marginTop="@dimen/_10sdp"
            app:placeholderImage="@color/color_base_grey"
            app:roundedCornerRadius="@dimen/_8sdp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
